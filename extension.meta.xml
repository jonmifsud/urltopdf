<?xml version="1.0" encoding="UTF-8"?>
<extension id="urltopdf" status="experimental" xmlns="http://symphony-cms.com/schemas/extension/1.0">
	<name>URL to PDF</name>
	<description>Generate a PDF from your Symphony page</description>
	<repo type="github">https://github.com/brendo/urltopdf</repo>
	<url type="discuss">http://symphony-cms.com/discuss/thread/73527/</url>
	<types>
		<type>Multimedia</type>
	</types>
	<authors>
		<author>
			<name github="brendo" symphony="brendo">Brendan Abbott</name>
			<website>http://www.bloodbone.ws</website>
		</author>
		<author>
			<name github="mazedigital" symphony="gunglien">Jon Mifsud (Maze Digital)</name>
			<website>https://maze.digital</website>
		</author>
	</authors>
	<releases>
		<release version="0.1" date="2011-07-13" min="2.2" max="2.2.x" />
		<release version="0.2" date="2017-06-19" min="2.3.0" max="2.6.x">
			- Changed Library to mPDF
			- Added H1,H2,H3 Support to create index tables
			- Added ability to append PDFs (on filesystem) to the one you want to generate
		</release>
		<release version="0.3" date="2017-07-05" min="2.3.0" max="2.6.x">
			- Update mpdf library to 7.x
		</release>
	</releases>
</extension>